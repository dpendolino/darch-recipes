#!/bin/bash
set -e

. ../common

# just in case google-chrome fails
install_aur_packages google-chrome || install_packages chromium

install_aur_packages \
  google-cloud-sdk \
  beautysh \

  install_packages \
  atom \
  autopep8 \
  zim \
  vagrant \
  packer-io \
  shellcheck \
  php \
  composer  \
  ccache \
